{
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/cron/notifications",
      "schedule": "0 6 * * *"
    }, {
      "path": "/api/cron/notifications",
      "schedule": "0 23 * * *"
    }
  ]
}